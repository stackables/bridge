{
  "name": "@stackables/bridge-syntax-highlight",
  "displayName": "The Bridge Language",
  "description": "Syntax highlighting for the bridge language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "bridge",
      "aliases": ["The Bridge", "bridge"],
      "extensions": [".bridge"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bridge",
      "scopeName": "source.bridge",
      "path": "./syntaxes/bridge.tmLanguage.json"
    }]
  }
}
